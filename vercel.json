{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/src/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/client/index.html"
    }
  ],
  "functions": {
    "server/src/index.ts": {
      "includeFiles": [
        "server/prisma/schema.prisma"
      ]
    }
  }
}